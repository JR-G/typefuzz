{
  "name": "typefuzz",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "TypeScript fuzz/property testing utilities with test runner integrations",
  "keywords": ["fuzz", "property-based", "testing", "vitest", "jest", "typescript"],
  "license": "MIT",
  "author": "",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./vitest": {
      "types": "./dist/vitest.d.ts",
      "default": "./dist/vitest.js"
    },
    "./jest": {
      "types": "./dist/jest.d.ts",
      "default": "./dist/jest.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
