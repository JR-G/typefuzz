{
  "name": "typefuzz",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "TypeScript fuzz/property testing utilities with test runner integrations",
  "keywords": ["fuzz", "property-based", "testing", "vitest", "jest", "typescript"],
  "license": "MIT",
  "author": "",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./zod": {
      "types": "./dist/zod.d.ts",
      "default": "./dist/zod.js"
    },
    "./vitest": {
      "types": "./dist/vitest.d.ts",
      "default": "./dist/vitest.js"
    },
    "./jest": {
      "types": "./dist/jest.d.ts",
      "default": "./dist/jest.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist", "README.md", "LICENSE"],
  "sideEffects": false,
  "engines": {
    "node": ">=18",
    "bun": ">=1.3.0"
  },
  "scripts": {
    "build": "bunx tsc -p tsconfig.build.json",
    "dev": "bunx tsc -p tsconfig.build.json --watch",
    "clean": "rm -rf dist",
    "lint": "bunx eslint .",
    "prepack": "bun run build",
    "test": "bunx vitest run"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "vitest": ">=1.0.0",
    "jest": ">=29.0.0",
    "zod": "^3.22.4"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    },
    "jest": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  }
}
